{
  "name": "@feedmob/google-drive-files",
  "version": "0.0.1",
  "description": "MCP Server for querying Google Drive files from Femini system",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "google-drive-files": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "mcp",
    "google-drive",
    "femini",
    "files"
  ],
  "author": "FeedMob",
  "homepage": "https://github.com/feed-mob/fm-mcp-servers",
  "bugs": "https://github.com/feed-mob/fm-mcp-servers/issues",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "dotenv": "^17.2.3",
    "fastmcp": "^3.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "shx": "^0.3.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
